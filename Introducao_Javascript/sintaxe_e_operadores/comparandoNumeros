var numero1 = 10;
var numero2 = 20;


function compararNumeros(num1, num2) {
    const primeiro = saoIguais(num1, num2);
    const segundo = somaMenorMaior(num1, num2);
    return `${primeiro} ${segundo}`;
}

function saoIguais(num1, num2) {
    return (num1 === num2) ? console.log("Os Números são iguais") : console.log("O número " + num1 + " não é igual o número " + num2);
}

function somaMenorMaior(num1, num2) {

    if (num1 += num2 > 10 || num1 + num2 < 20) {
        return console.log("A Esta dentro do intervalo esperado");
    }

    return console.log("A soma Não está dentro do intervalo esperado");
}

console.log(compararNumeros(numero1, numero2));